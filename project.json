{
    "name": "Gay-I Club Concierge",
    "type": "web-app",
    "description": "AI-powered community concierge platform for the Gay-I Club. Features event management, RSVPs, community resources, member profiles, and an AI chatbot assistant.",
    "created": "2025-12-16",
    "vibe": "cyberpunk-futuristic",
    "features": [
        "User authentication (Email, Magic Link, Google OAuth)",
        "Event management with RSVP system",
        "Community resource library",
        "Member profiles and interests",
        "AI-powered chat assistant",
        "Calendar integration (iCal, Google Calendar)",
        "Event alerts and notifications"
    ],
    "stack": {
        "backend": "Next.js 16 (App Router)",
        "frontend": "React 19 + TypeScript",
        "database": "Supabase (PostgreSQL)",
        "styling": "Tailwind CSS",
        "ai": "OpenAI"
    },
    "urls": {
        "production": "https://gayiclub.com",
        "repo": "https://github.com/Guatapickl/gay-i-concierge"
    }
}